<h1>Editing team</h1>

<% form_for(@team, :url => team_url) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :slug %><br />
    <%= f.text_field :slug %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', team_path %>
